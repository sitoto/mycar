<%= simple_form_for(@qqcar) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :name_pinyin %>
    <%= f.input :brand %>
    <%= f.input :series %>
    <%= f.input :maker %>
    <%= f.input :url %>
    <%= f.input :price %>
    <%= f.input :all_price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
